doctype html
html(lang='zh_cn')
    head
        title #{title}
        include ./../../command/meta
        link(rel="stylesheet" href="../weui/dist/style/weui.min.css")
        style.
            html {
                background: #EEE;
            }
            body{
                font-size:14px !important;
                font-family: "Helvetica Neue", Helvetica, Tahoma, Arial, 'Microsoft Yahei', 'Hiragino Sans GB', 'WenQuanYi Micro Hei', sans-serif !important;
            }
            img{
                width:30px;margin-right:5px;display:none;
            }
            .feedback{
                color: #b32020;
                text-align: center;
                width: 100%;
                display: block;
                margin-top: 30px;
                cursor: pointer;
            }
            .weui_cell p{
                font-size: 15px !important;
            }
    body
        div.weui_cells_title  问题类型
        div.weui_cells.weui_cells_access
            a.weui_cell(href="help/help_detail?id=12&title=注册与登录")
                div.weui_cell_hd
                    img(src="data:image/png;base64,")
                div.weui_cell_bd.weui_cell_primary
                    p 注册与登录
                div.weui_cell_ft
            a.weui_cell(href="help/help_detail?id=13&title=充值与提现")
                div.weui_cell_hd
                    img(src="data:image/png;base64,")
                div.weui_cell_bd.weui_cell_primary
                    p 充值与提现
                div.weui_cell_ft
            a.weui_cell(href="help/help_detail?id=15&title=账户与安全")
                div.weui_cell_hd
                    img(src="data:image/png;base64,")
                div.weui_cell_bd.weui_cell_primary
                    p 账户与安全
                div.weui_cell_ft
        div.weui_cells.weui_cells_access(style="margin-top: 0.5em;")
            a.weui_cell(href="help/help_detail?id=14&title=投资与还款")
                div.weui_cell_hd
                    img(src="data:image/png;base64,")
                div.weui_cell_bd.weui_cell_primary
                    p 投资与还款
                div.weui_cell_ft
            a.weui_cell(href="help/help_detail?id=16&title=融资与借款")
                div.weui_cell_hd
                    img(src="data:image/png;base64,")
                div.weui_cell_bd.weui_cell_primary
                    p 融资与借款
                div.weui_cell_ft
            a.weui_cell(href="help/help_detail?id=1&title=一般问题")
                div.weui_cell_hd
                    img(src="data:image/png;base64,")
                div.weui_cell_bd.weui_cell_primary
                    p 一般问题
                div.weui_cell_ft
        a(href="/feedback/add" class="feedback") 意见反馈